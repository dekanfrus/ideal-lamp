<Window x:Class="ChatServer1.ChatWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChatServer1"
        mc:Ignorable="d"
        Title="J²A Chat Services" Height="300" Width="521.008">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="222" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="416" IsReadOnly="True"/>
        <TextBox x:Name="chat_window_message_input" HorizontalAlignment="Left" Height="23" Margin="10,237,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="336"/>
        <Button x:Name="chat_window_submit_message" Click="chat_window_submit_message_Click" Content="Submit" HorizontalAlignment="Left" Margin="351,238,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox x:Name="chatwindow_users_list_" HorizontalAlignment="Left" Height="188" Margin="431,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="82"/>
        <Label x:Name="chatwindow_user_list_label" Content="Users" HorizontalAlignment="Left" Margin="431,13,0,0" VerticalAlignment="Top" Width="82" RenderTransformOrigin="0.856,0.407"/>
    </Grid>
</Window>
